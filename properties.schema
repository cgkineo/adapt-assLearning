{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "pluginLocations": {
      "type": "object",
      "properties": {
        "component": {
          "type": "object",
          "properties": {
            "_assLearning": {
              "type": "array",
              "default": [],
              "legend": "Associated Learning",
              "inputType": "List",
              "help": "Associated learning is listed for incorrectly answered questions with linked IDs. In the body text for the Assessment Results component, add the reference `{{{_assLearning}}}`"
            }
          }
        }
      }
    }
  }
}
